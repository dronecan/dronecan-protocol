<?xml version="1.0"?>

<Protocol name="DroneCAN_Packets" file="dronecan_packets">

<Enum name="DroneCAN_Param_Packets" file="dronecan_packets" lookup="true" lookupTitle="true" prefix="PKT_DC_PARAM_">
    <!-- Device parameters -->
    <Value name="REQUEST" value="0xEF00" comment="Request device parameter"/>
    <Value name="INFO" comment="Parameter description"/>
    <Value name="VALUE" comment="Parameter value"/>
</Enum>

<Enum name="DroneCAN_System_Packets" file="dronecan_packets" lookup="true" lookupTitle="true" prefix="PKT_DC_SYS_">
    
    <!-- System information packets -->
    <Value name="STATUS" value="0xF000" comment="Device status information"/>

    <Value name="UID" value="0xF100" comment="Unique Identifer for DroneCAN node"/>

    <Value name="MANF_STRING" value="0xF105" comment="Manufacturer device identifier string"/>
    <Value name="USER_STRING" comment="User device identifier string"/>

    <Value name="FW_VERSION" value="0xF110" comment="Firmware version information"/>
    <Value name="FW_DATE" comment="Firmware date information"/>
    <Value name="FW_SETTINGS" comment="User-configurable settings information"/>

    <Value name="HW_INFO" value="0xF115" comment="Hardware information"/>

    <Value name="PROTOCOL_INFO" value="0xF11A" comment="DroneCAN protocol information"/>


</Enum>

</Protocol>